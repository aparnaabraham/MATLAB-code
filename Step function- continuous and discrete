clear all;
close all;
clc;
N=20;
n=0:1:N-1;
y=ones(1,N);
subplot(2,1,1);
plot(n,y);
ylabel('amplitude');
xlabel('time--->');
title('continuous unit step signal');
display(y);
subplot(2,1,2);
stem(n,y);
ylabel('amplitude');
xlabel('number of samples--->');
title('discrete unit step signal');
display(y);
